	doi = {10.1016/j.chemosphere.2023.137903},
	abstract = {Per- and polyfluoroalkyl substances (PFAS), found in many consumer products, are commonly disposed of in landfills at the end of their service lives. To identify landfill liquids that should be prioritized for treatment, this study aimed to evaluate PFAS levels in different aqueous samples from landfills and identify relationships between PFAS and landfill characteristics. Twenty-six PFAS including 11 perfluoroalkyl carboxylic acids (PFCAs), 7 perfluoroalkyl sulfonates (PFSAs), and 8 perfluoroalkyl acid precursors (PFAA-precursors) were measured in municipal solid waste (MSW) leachate, construction and demolition debris (CDD) leachate, municipal solid waste incineration (MSWI) ash leachate, gas condensate, stormwater, and groundwater from landfills. Based on the median, results show that PFAS levels in MSW leachate were the highest (10,000 ng L−1), CDD leachate were intermediate (6200 ng L−1), and MSWI ash leachate were the lowest (1300 ng L−1) among the leachates evaluated. PFAS levels in gas condensate (7000 ng L−1) were similar to MSW leachate. PFAS in stormwater and groundwater were low (medians were less than 500 ng L−1). Dominant subgroups included PFCAs and PFAA-precursors in all leachates. PFSAs were also found in CDD leachate, PFAA-precursors in gas condensate, and PFCAs in stormwater and groundwater. Landfill characteristics significantly correlated with ∑26PFAS included waste proportions (percentage of MSWI ash in landfill, {\textbar}rs{\textbar} = 0.22), operational status (active or not, {\textbar}rs{\textbar} = 0.27) and rainfall (30-d cumulative rainfall, {\textbar}rs{\textbar} = 0.39). The results from this study can be used to prioritize which landfills and which reservoir of liquids (and corresponding subgroup of PFAS) to target for PFAS management.},
	language = {en},
	urldate = {2023-06-12},
	journal = {Chemosphere},
	author = {Chen, Yutao and Zhang, Hekai and Liu, Yalan and Bowden, John A. and Tolaymat, Thabet M. and Townsend, Timothy G. and Solo-Gabriele, Helena M.},
	month = mar,
	year = {2023},
	keywords = {PFAS, Gas condensate, Groundwater, Landfill leachate, PFAA-Precursors, Stormwater},
	pages = {137903},
	file = {ScienceDirect Snapshot:/home/grace/Zotero/storage/4IBIV9QB/S0045653523001704.html:text/html},
}

@article{carlson_systematic_2022,
