	author = {Ankley, Gerald T. and Cureton, Philippa and Hoke, Robert A. and Houde, Magali and Kumar, Anupama and Kurias, Jessy and Lanno, Roman and McCarthy, Chris and Newsted, John and Salice, Christopher J. and Sample, Bradley E. and Sepúlveda, Maria S. and Steevens, Jeffery and Valsecchi, Sara},
	month = mar,
	year = {2021},
	pmid = {32897586},
	pmcid = {PMC7984443},
	note = {Number: 3},
	keywords = {Ecological risk, Per- and poly-fluoroalkyl substances, Review, Strategy},
	pages = {564--605},
	file = {Full Text:/home/grace/Zotero/storage/3IB7JM4L/Ankley et al. - 2021 - Assessing the Ecological Risks of Per- and Polyflu.pdf:application/pdf},
}

