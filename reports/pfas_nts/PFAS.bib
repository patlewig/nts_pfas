	pmid = {27560178},
	pmcid = {PMC5223290},
	note = {Number: 9},
	keywords = {Humans, Cell Line, Tumor, Cell Shape, Cell Size, Fluorescent Dyes, Image Processing, Computer-Assisted, Molecular Imaging, Staining and Labeling},
	pages = {1757--1774},
}

