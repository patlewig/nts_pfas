	keywords = {Humans, Biological Assay, Dose-Response Relationship, Drug, thyroid, Enzyme Inhibitors, Small Molecule Libraries, prioritization, HEK293 Cells, Iodides, Adenoviridae, deiodinase, endocrine disruption, Inhibitory Concentration 50, Iodide Peroxidase, screening, Transfection},
