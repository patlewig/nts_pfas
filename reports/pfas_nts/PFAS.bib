	keywords = {Animals, Humans, Toxicity Tests, Rats, Animal Testing Alternatives, High-Throughput Screening Assays, Computer Simulation, Models, Biological, United States, United States Environmental Protection Agency, Mice, Small Molecule Libraries, Phenotype},
	pages = {583--591},
}

