	author = {Yang, Chihae and Tarkhov, Aleksey and Marusczyk, Jörg and Bienfait, Bruno and Gasteiger, Johann and Kleinoeder, Thomas and Magdziarz, Tomasz and Sacher, Oliver and Schwab, Christof H. and Schwoebel, Johannes and Terfloth, Lothar and Arvidson, Kirk and Richard, Ann and Worth, Andrew and Rathman, James},
