	keywords = {Humans, Risk Assessment, Hazardous Substances, No-Observed-Adverse-Effect Level, Drug-Related Side Effects and Adverse Reactions, high-throughput screening, high-throughput toxicokinetics, new approach methodologies, point-of-departure (POD), threshold of toxicological concern (TTC)},
	pages = {202--225},
