	keywords = {Computer Simulation, Cheminformatics, Fluorocarbons},
	pages = {508--534},
	file = {Full Text:/home/grace/Zotero/storage/P4XPJ9YS/Richard et al. - 2023 - A New CSRML Structure-Based Fingerprint Method for.pdf:application/pdf},
}

