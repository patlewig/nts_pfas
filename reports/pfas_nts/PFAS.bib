	keywords = {Animals, Humans, Risk Assessment, Computational Biology, Biological Assay, High-Throughput Screening Assays, computational toxicology, Transcriptome, transcriptomics, high-throughput screening, TempO-Seq},
	pages = {68--89},
	file = {Accepted Version:/home/grace/Zotero/storage/FJH32V6S/Harrill et al. - 2021 - High-Throughput Transcriptomics Platform for Scree.pdf:application/pdf},
